  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "publish": "electron-builder --win -p always",
    "release": "build"
  },
  "keywords": [
    "Electron",
    "quick"
  ],
  "author": "IVAN ESTEBAN NESTERUK",
  "license": "CC0-1.0",
  "dependencies": {
