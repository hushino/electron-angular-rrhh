    "electron-updater": "^4.2.0"
  },
  "devDependencies": {
    "electron": "^13.6.6",
    "electron-builder": "22.1.0"
  },
  "build": {
    "appId": "rrhh.redmagic",
    "directories": {
      "buildResources": "public"
    },
    "extends": null,
    "win": {
      "publish": [
        "github"
